local Section = Tab4:AddSection({
	Name = "----------------------------------------------------------------------------------------------------------------------------------------------"
})
local Section = Tab4:AddSection({
	Name = "Magma"
})

Tab4:AddToggle({
    Name = "Magma Fists",
    Default = false,
    Callback = function(value)
        MonM = value
        MaTT = value
        if MonM then

            spawn(function()
                while MonM do
                    local MobBow = MonsterMagic()
                    if MobBow then
                        local bow = game.Players.LocalPlayer.Character.MagmaFists
                        local swordScript = bow:FindFirstChild("ServerScript")
                        if swordScript then
                            local targetPos = MobBow.HumanoidRootPart.Position 
                            swordScript.Start:FireServer()
                            local targetPos = MobBow.HumanoidRootPart.Position 
                            swordScript.Shoot:FireServer(targetPos)
                        end
                    end
                    wait(0.1)
                end
            end)
            
        spawn(function()
            while MaTT do
                local MobBow = MonsterMagic()
                if MobBow then
                TweenService:Create(player.Character.HumanoidRootPart, TweenInfoClose, {CFrame = MobBow.HumanoidRootPart.CFrame * CFrame.new(0,-25,50)}):Play()
                        wait(1)
                    end
                    wait()
                end
            end)
        end
    end
})

