--------------------------------------------------------------------------------------------------------------------------------------------------------------------

local Section = Tab2:AddSection({
	Name = "Bow Farm All Lv.50"
})
local Section = Tab2:AddSection({
	Name = "If using Save HP, open infJume"
})
-------------------------------------------------------------------------------------------------------------------------------------------------------------------
---Save HP Gigy
local MonZK50 = {"Lahmu","MysticIllusionist","DesertSnake","Enkidu","Gilgamesh"}

local Mon = true
local BowTT = true

local function MonsterZK50()
    local closestDist = math.huge
    local MobBow = nil
    for i,v in pairs(game.workspace.Mobs:GetChildren()) do
        if v:FindFirstChild("HumanoidRootPart") and v.Health.Value > 0 then
            local dist = (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude 
                if dist < closestDist then
                    closestDist = dist
                    MobBow = v
                end
            end
        end
        return MobBow
    end

--------------------------------------------------------------------------------------------------------------------------------------------------------------------    

Tab2:AddToggle({
    Name = "Bow Save HP",
    Default = false,
    Callback = function(value)
        Mon = value
        if Mon then
            spawn(function()
                while Mon do
                    local MobBow = MonsterZK50()
                    if MobBow then
                        local bow = game.Players.LocalPlayer.Character.Bow
                        local swordScript = bow:FindFirstChild("SwordScript")
                        if swordScript then
                            local targetPos = MobBow.HumanoidRootPart.Position 
                            swordScript.Shoot:FireServer(targetPos, 21)
                        end
                    end
                    wait(0.1)
                end
            end)
        end
    end
})

--------------------------------------------------------------------------------------------------------------------------------------------------------------------

---No Save Hp Gigy
Tab2:AddToggle({
    Name = "Bow No Save Hp",
    Default = false,
    Callback = function(value)
        Mon = value
        BowTT = value
        if Mon then

            spawn(function()
                while Mon do
                    local MobBow = MonsterZK50()
                    if MobBow then
                        local bow = game.Players.LocalPlayer.Character.Bow
                        local swordScript = bow:FindFirstChild("SwordScript")
                        if swordScript then
                            local targetPos = MobBow.HumanoidRootPart.Position 
                            swordScript.Shoot:FireServer(targetPos, 21)
                        end
                    end
                    wait(0.1)
                end
            end)
            
        spawn(function()
            while BowTT do
                local MobBow = MonsterZK50()
                if MobBow then
                TweenService:Create(player.Character.HumanoidRootPart, TweenInfoClose, {CFrame = MobBow.HumanoidRootPart.CFrame * CFrame.new(0,0,50)}):Play()
                        wait(1)
                    end
                    wait()
                end
            end)
        end
    end
})

----------------------------------------------------------------------------------------------------------------------------------------------

local Section = Tab2:AddSection({
	Name = "----------------------------------------------------------------------------------------------------------------------------------------------"
})
local Section = Tab2:AddSection({
	Name = "Zaphkiel Farm All Lv.50"
})
local Section = Tab2:AddSection({
	Name = "Guide to use guns from World Boss 2 requires"
})
----------------------------------------------------------------------------------------------------------------------------------------------

---Save Hp All Mob
local MonZK50 = {"Lahmu","MysticIllusionist","DesertSnake","Enkidu","Gilgamesh"}

local MonZK = true
local ZKTT = true

local function MonsterZK50()
    local closestDist = math.huge
    local MobZK = nil
    for i,v in pairs(game.workspace.Mobs:GetChildren()) do
        if v:FindFirstChild("HumanoidRootPart") and v.Health.Value > 0 then
            local dist = (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude 
                if dist < closestDist then
                    closestDist = dist
                    MobZK  = v
                end
            end
        end
        return MobZK 
    end

-----------------------------------------------------------------------------------------------------------------------------------------------

Tab2:AddToggle({
    Name = "Zaphkiel No TP",
    Default = false,
    Callback = function(value)
        MonZK = value
        ZKTT = value
        if MonZK then

            spawn(function()
                while MonZK do
                    local MobZK = MonsterZK50()
                    if MobZK then
                        local zaphkiel = game.Players.LocalPlayer.Character.Zaphkiel
                        local ServerScript = zaphkiel:FindFirstChild("ServerScript")
                        if ServerScript then
                            local targetPos = MobZK.HumanoidRootPart.Position 
                            ServerScript.Create:FireServer(targetPos)
                        end
                    end
                    wait(0.1)
                end
            end)
        end
    end
})

----------------------------------------------------------------------------------------------------------------------------------------------

---No Save Hp All Mob

Tab2:AddToggle({
    Name = "Zaphkiel TP",
    Default = false,
    Callback = function(value)
        MonZK = value
        ZKTT = value
        if MonZK then

            spawn(function()
                while MonZK do
                    local MobZK = MonsterZK50()
                    if MobZK then
                        local zaphkiel = game.Players.LocalPlayer.Character.Zaphkiel
                        local ServerScript = zaphkiel:FindFirstChild("ServerScript")
                        if ServerScript then
                            local targetPos = MobZK.HumanoidRootPart.Position 
                            ServerScript.Create:FireServer(targetPos)
                        end
                    end
                    wait(0.1)
                end
            end)
            
            spawn(function()
                while ZKTT do
                    local MobZK = MonsterZK50()
                    if MobZK then
                        TweenService:Create(player.Character.HumanoidRootPart, TweenInfoClose, {CFrame = MobZK.HumanoidRootPart.CFrame * CFrame.new(0,0,50)}):Play()
                        wait(1)
                    end
                    wait()
                end
            end)
        end
    end
})

