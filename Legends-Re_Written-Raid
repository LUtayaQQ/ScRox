Tab4:AddToggle({
    Name = "Water Pulse",
    Default = false,
    Callback = function(value)
        MonM = value
        MaTT = value
        if MonM then

            spawn(function()
                while MonM do
                    local MobBow = MonsterMagic()
                    if MobBow then
                        local bow = game.Players.LocalPlayer.Character.WaterPulse
                        local swordScript = bow:FindFirstChild("ServerScript")
                        if swordScript then
                            local targetPos = MobBow.HumanoidRootPart.Position 
                            swordScript.Charge:FireServer(targetPos)
                        end
                        wait(0.01)
                        local bow = game.Players.LocalPlayer.Character.WaterPulse
                        local swordScript = bow:FindFirstChild("ServerScript")
                        if swordScript then
                            local targetPos = MobBow.HumanoidRootPart.Position 
                            swordScript.Throw:FireServer(targetPos)
                        end
