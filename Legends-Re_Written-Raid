local Section = Tab4:AddSection({
	Name = "----------------------------------------------------------------------------------------------------------------------------------------------"
})
local Section = Tab4:AddSection({
	Name = "Light"
})


Tab4:AddToggle({
    Name = "Light Beam",
    Default = false,
    Callback = function(value)
        MonM = value
        MaTT = value
        if MonM then

            spawn(function()
                while MonM do
                    local MobBow = MonsterMagic()
                    if MobBow then
                        local bow = game.Players.LocalPlayer.Character.LightBeam
                        local swordScript = bow:FindFirstChild("ServerScript")
                        if swordScript then
                            local targetPos = MobBow.HumanoidRootPart.Position 
                            swordScript.Charge:FireServer(targetPos)
                        end
                    end
                    wait(0.01)
                end
            end)
            
        spawn(function()
            while MaTT do
                local MobBow = MonsterMagic()
                if MobBow then
                TweenService:Create(player.Character.HumanoidRootPart, TweenInfoClose, {CFrame = MobBow.HumanoidRootPart.CFrame * CFrame.new(0,-25,50)}):Play()
                        wait(1)
                    end
                    wait()
                end
            end)
        end
    end
})

