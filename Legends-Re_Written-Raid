-------------------------------------------------------------------------------------------------------------------------------------------------------------------
---Save HP Gigy

local MonM = true
local MaTT = true

local function MonsterMagic()
    local closestDist = math.huge
    local MobBow = nil
    for i,v in pairs(game.workspace.Mobs:GetChildren()) do
        if v:FindFirstChild("HumanoidRootPart") and v.Health.Value > 0 then
            local dist = (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude 
                if dist < closestDist then
                    closestDist = dist
                    MobBow = v
                end
            end
        end
        return MobBow
    end

--------------------------------------------------------------------------------------------------------------------------------------------------------------------

local Section = Tab4:AddSection({
	Name = "----------------------------------------------------------------------------------------------------------------------------------------------"
})
local Section = Tab4:AddSection({
	Name = "Wind"
})

Tab4:AddToggle({
    Name = "Razor Wind",
    Default = false,
    Callback = function(value)
        MonM = value
        MaTT = value
        if MonM then

            spawn(function()
                while MonM do
                    local MobBow = MonsterMagic()
                    if MobBow then
                        local bow = game.Players.LocalPlayer.Character.RazorWind
                        local swordScript = bow:FindFirstChild("ServerScript")
                        if swordScript then
                            local targetPos = MobBow.HumanoidRootPart.Position 
                            swordScript.Charge:FireServer(targetPos)
                        end
                    end
                    wait(0.1)
                end
            end)
            
        spawn(function()
            while MaTT do
                local MobBow = MonsterMagic()
                if MobBow then
                TweenService:Create(player.Character.HumanoidRootPart, TweenInfoClose, {CFrame = MobBow.HumanoidRootPart.CFrame * CFrame.new(0,0,6)}):Play()
                        wait(1)
                    end
                    wait()
                end
            end)
        end
    end
})

Tab4:AddToggle({
    Name = "Wind Blade",
    Default = false,
    Callback = function(value)
