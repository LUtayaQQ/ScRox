---Legendary Super Level:200
local MobName = "Legendary"
local MobName1 = "LegandarySuper"

Tab4:AddToggle({
    Name = "Legendary Level:200",
    Default = false,
    Callback = function(value)
        Mon = value
        BowAttack = value
        if Mon then
            
        moveTo(CFrame.new(-908.38720703125, 154.69476318359375, -1596.7698974609375))
            
-- Find and attack the "Legendary" mob
local targetMob = nil
for i, v in ipairs(Workspace.Mobs:GetChildren()) do
    if string.match(v.Name, MobName) and v:FindFirstChild("HumanoidRootPart") and v.HumanoidRootPart.Position.Z - char.HumanoidRootPart.Position.Z <= 500 then
        targetMob = v
        break
    end
end

if targetMob ~= nil then
    spawn(function()
        while Mon and targetMob.Humanoid.Health > 0 do
            TweenService:Create(player.Character.HumanoidRootPart, TweenInfoClose, {CFrame = targetMob.HumanoidRootPart.CFrame * CFrame.new(0,0,20)}):Play()
            wait(0.5)
        end
    end)
        
    spawn(function()
        while BowAttack and targetMob.Humanoid and targetMob.Humanoid.Health > 0 do
            char.Bow.SwordScript.Shoot:FireServer(targetMob.HumanoidRootPart.CFrame.Position, 21)
            wait(0.1)
        end
    end)
end

-- Find and attack the "LegendarySuper" mob
local targetMob2 = nil
for i, v in ipairs(Workspace:GetChildren()) do
    if string.match(v.Name, MobName1) and v:FindFirstChild("HumanoidRootPart") and v.HumanoidRootPart.Position.Z - char.HumanoidRootPart.Position.Z <= 500 then
        targetMob2 = v
        break
    end
end

if targetMob2 ~= nil then
    spawn(function()
        while Mon and targetMob2.Humanoid.Health > 0 do
            TweenService:Create(player.Character.HumanoidRootPart, TweenInfoClose, {CFrame = targetMob2.HumanoidRootPart.CFrame * CFrame.new(0,0,20)}):Play()
            wait(0.5)
        end
    end)
        
    spawn(function()
        while BowAttack and targetMob2.Humanoid and targetMob2.Humanoid.Health > 0 do
            char.Bow.SwordScript.Shoot:FireServer(targetMob2.HumanoidRootPart.CFrame.Position, 21)
            wait(0.1)
        end
    end)
end
repeat wait() until w.Humanoid.Health <= 0
    wait(1)
    moveTo(w.HumanoidRootPart.CFrame)
    wait(4)
    moveTo(CFrame.new(-908.38720703125, 154.69476318359375, -1596.7698974609375))
    wait(2)
    end
end
})

