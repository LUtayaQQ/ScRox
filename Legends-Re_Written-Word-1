    Name = "Gravity",
    Default = false,
    Callback = function(value)
        MonM = value
        MaTT = value
        if MonM then

            spawn(function()
                while MonM do
                    local MobBow = MonsterMagic()
                    if MobBow then
                        local bow = game.Players.LocalPlayer.Character.Gravity
                        local swordScript = bow:FindFirstChild("ServerScript")
                        if swordScript then
                            local targetPos = MobBow.HumanoidRootPart.Position 
                            swordScript.Charge:FireServer(targetPos)
                        end
                    end
                    wait(0.1)
                end
            end)
        end
    end
})

