--local TweenService
local TweenService = game:GetService("TweenService")
local TweenInfoClose = TweenInfo.new(0.5, Enum.EasingStyle.Linear)
local Character = game.Players.LocalPlayer.Character


--local Players
local Players = game:GetService("Players")
local player = game:GetService("Players").LocalPlayer
local char = Players.LocalPlayer.Character
local HumanoidRootPart = Character.HumanoidRootPart

--local WSH
local Workspace = game:GetService("Workspace")

---------------------------------------------------------------------------------------------------------------------------------------

local InfJump

Tab1:AddToggle({
    Name = "InfJump",
    Default = false,
    Callback = function(value)
        InfJump = value
    end
})

local function onJumpRequest()
    if InfJump then
        game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
    end
end

game:GetService("UserInputService").JumpRequest:connect(onJumpRequest)

-----------------------------------------------------------------------------------------------------------------------------------

local Speed1 = 100

Tab1:AddButton({
    Name = "Speed 100",
    Callback = function(value)
        if Speed1 then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Speed1
        end
    end
})

-----------------------------------------------------------------------------------------------------------------------------------

local Speed = 0
Tab1:AddTextbox({
    Name = "Speed Edit",
    Default = "      ",
    TextDisappear = true,
    Callback = function(value)
        local amount = tonumber(value)
        if amount and amount > 0 then
            Speed = amount
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Speed
        end
    end
})

------------------------------------------------------------------------------------------------------------------------------------

local RuneArrows = true

Tab1:AddTextbox({
    Name = "Buy RuneArrows",
    Default = "      ",
    TextDisappear = true,
    Callback = function(value)
        local amount = tonumber(value)
        if amount and amount > 0 and RuneArrows then
            local ohString1 = "RuneArrows"
            local ohString2 = "Bows"
            local ohNumber3 = amount
            game:GetService("ReplicatedStorage").Remotes.BuyItem:FireServer(ohString1, ohString2, ohNumber3)
        end
    end
})

------------------------------------------------------------------------------------------------------------------------------------

local AutoFish

Tab1:AddToggle({
    Name = "Auto Fish",
    Default = false,
        Callback = function(value)
        AutoFish = value
        if AutoFish then
                while AutoFish do
                local ohInstance1 = workspace.FishingSpawns.Fishing
                game:GetService("Players").LocalPlayer.PlayerGui.Fish.Ado.catch:FireServer(ohInstance1)
                wait(5)
            end
        end
    end
})

---------------------------------------------------------------------------------------------------------------------------------------


local Dash

Tab1:AddToggle({
    Name = "Farm Speed",
    Default = false,
        Callback = function(value)
        Dash = value
        if Dash then
                while Dash do
                game:GetService("Players").LocalPlayer.PlayerGui.Parkour.Script.Dash:FireServer()
                wait(0.5)
            end
        end
    end
})

------------------------------------------------------------------------------------------------------------------------------------

---local shop
local SMB = true

local function TPSMB(destination)
    local HumanoidRootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
    HumanoidRootPart.Anchored = true
    
    local distance = (destination.Position - HumanoidRootPart.Position).Magnitude
    local speed = 500
    local time = distance / speed
    
    local TweenService = game:GetService("TweenService")
    local tweenInfo = TweenInfo.new(time, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
    local tween = TweenService:Create(HumanoidRootPart, tweenInfo, {CFrame = destination})
    tween:Play()
    HumanoidRootPart.Anchored = false
end

-------------------------------------------------------------------------------------------------------------------------------------

Tab1:AddButton({
    Name = "Shop",
    Callback = function(value)
        if SMB then
            TPSMB(CFrame.new(-841.3690795898438, -86.04965209960938, 404.4039306640625))
        end
    end
})

------------------------------------------------------------------------------------------------------------------------------------

Tab1:AddButton({
    Name = "BlackSmith",
    Callback = function(value)
        if SMB then
            TPSMB(CFrame.new(-896.8595581054688, -75.85083770751953, 402.48199462890625))
        end
    end
})

