local Section = Tab4:AddSection({
	Name = "If you press Farm boss and it doesn't work, press Bypass."
})

--local Farm 
local function moveToBP(destination)
    
    local distance = (destination.Position - HumanoidRootPart.Position).Magnitude
    local speed = 500
    local time = distance / speed
    
    local TweenService = game:GetService("TweenService")
    local tweenInfo = TweenInfo.new(time, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
    local tween = TweenService:Create(HumanoidRootPart, tweenInfo, {CFrame = destination})
    tween:Play()
    tween.Completed:wait()
end

-------------------------------------------------------------------------------------------------------------------------------------------------------------------

local BypassGG = true

Tab4:AddButton({
    Name = "Bypass",
    Callback = function(value)
        if BypassGG then
    
            moveToBP(CFrame.new(-2938.394775390625, 48.940223693847656, -1075.271484375))
            wait(1)
            moveToBP(CFrame.new(-701.3289184570312, 263.4222106933594, -497.8858337402344))
            wait(1)
            moveToBP(CFrame.new(292.1370544433594, 19.368431091308594, -391.00445556640625))
            wait(1)
            moveToBP(CFrame.new(-715.0462646484375, 152.63833618164062, -1735.9384765625))
            wait(1)
            moveToBP(CFrame.new(-741.6810913085938, 99.91461944580078, 416.1151123046875))
        end
    end
})

