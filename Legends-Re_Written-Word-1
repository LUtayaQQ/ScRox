                                TweenService:Create(player.Character.HumanoidRootPart, TweenInfoClose, {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,3)}):Play()
                                wait(0.5)
                            end
                        end)
                            
                            spawn(function()
                                while Mon and v.Humanoid and v.Humanoid.Health > 0 do
                                    char.DualSwords.SwordScript.Activate:FireServer(v.HumanoidRootPart.Position)
                                    wait(0.1)
                                    end
                                end)
                            
                        repeat wait() until v.Humanoid.Health <= 0
                        moveTo(v.HumanoidRootPart.CFrame)
                        wait(4)
                        moveTo(CFrame.new(-2899.255859375, 332.9498291015625, -865.3580932617188))
                        wait(4)
                        moveTo(CFrame.new(-3119.896484375, 463.95001220703125, -1144.848876953125))
                        wait(2)
                    end
                end
                wait(0.5)
            end
        end
    end
})

