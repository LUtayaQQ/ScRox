--local TweenService
local TweenService = game:GetService("TweenService")
local TweenInfoClose = TweenInfo.new(0.5, Enum.EasingStyle.Linear)
local Character = game.Players.LocalPlayer.Character


--local Players
local Players = game:GetService("Players")
local player = game:GetService("Players").LocalPlayer
local char = Players.LocalPlayer.Character
local HumanoidRootPart = Character.HumanoidRootPart

--local WSH
local Workspace = game:GetService("Workspace")

----------------------------------------------------------------------------------------------------------------------------------------
InfJump = false
Tab1:AddToggle({
    Name = "InfJump",
    Default = false,
    Callback = function(value)
        InfJump = value
    end
})

local function onJumpRequest()
    if InfJump then
        game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
    end
end

game:GetService("UserInputService").JumpRequest:connect(onJumpRequest)

--------------------------------------------------------------------------------------------------------------------------------


local isTesting = game:GetService("RunService"):IsStudio()
local originalWaterTextures
if isTesting then
    originalWaterTextures = nil
else
    originalWaterTextures = game:GetService("Workspace"):FindFirstChild("WaterTextures")
end
local isWaterHidden = false

Tab1:AddToggle({
    Name = "Hide Water",
    Default = false,
    Callback = function(value)
        isWaterHidden = value

        if isWaterHidden == true then
            if originalWaterTextures ~= nil then
                for _, child in ipairs(originalWaterTextures:GetChildren()) do
                    child.Transparency = 1 -- ซ่อน part ที่อยู่ใน WaterTextures ทั้งหมด
                end
            end
            
            isWaterHidden = true
        
        else
            if originalWaterTextures ~= nil then
                for _, child in ipairs(originalWaterTextures:GetChildren()) do
                    child.Transparency = 0 -- แสดง part ที่อยู่ใน WaterTextures ทั้งหมด
                end
                isWaterHidden = false
            else
                originalWaterTextures = Instance.new("Folder", game:GetService("Workspace"))
                originalWaterTextures.Name = "WaterTextures"
                isWaterHidden = false
            end
        end
    end
})

-----------------------------------------------------------------------------------------------------------------------------------

local Speed1 = 100

Tab1:AddButton({
    Name = "Speed 100",
    Callback = function(value)
        if Speed1 then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Speed1
        end
    end
})

-----------------------------------------------------------------------------------------------------------------------------------

local Speed = 0
Tab1:AddTextbox({
    Name = "Speed Edit",
    Default = "      ",
    TextDisappear = true,
    Callback = function(value)
        local amount = tonumber(value)
        if amount and amount > 0 then
            Speed = amount
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Speed
        end
    end
})

------------------------------------------------------------------------------------------------------------------------------------

local RuneArrows = true
Tab1:AddTextbox({
    Name = "Buy RuneArrows",
    Default = "      ",
    TextDisappear = true,
    Callback = function(value)
        local amount = tonumber(value)
        if amount and amount > 0 and RuneArrows then
            local ohString1 = "RuneArrows"
            local ohString2 = "Bows"
            local ohNumber3 = amount
            game:GetService("ReplicatedStorage").Remotes.BuyItem:FireServer(ohString1, ohString2, ohNumber3)
        end
    end
})

------------------------------------------------------------------------------------------------------------------------------------

AutoFish = false

Tab1:AddToggle({
    Name = "Auto Fish",
    Default = false,
        Callback = function(value)
        AutoFish = value
        if AutoFish then
                while AutoFish do
                local ohInstance1 = workspace.FishingSpawns.Fishing
                game:GetService("Players").LocalPlayer.PlayerGui.Fish.Ado.catch:FireServer(ohInstance1)
                wait(5)
            end
        end
    end
})

---------------------------------------------------------------------------------------------------------------------------------------


Dash = false

Tab1:AddToggle({
    Name = "Farm Speed",
    Default = false,
        Callback = function(value)
        Dash = value
        if Dash then
                while Dash do
                game:GetService("Players").LocalPlayer.PlayerGui.Parkour.Script.Dash:FireServer()
                wait(0.5)
            end
        end
    end
})

------------------------------------------------------------------------------------------------------------------------------------

local SMB = true

local function TPSMB(destination)
    local HumanoidRootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
    HumanoidRootPart.Anchored = true
    
    local distance = (destination.Position - HumanoidRootPart.Position).Magnitude
    local speed = 500
    local time = distance / speed
    
    local TweenService = game:GetService("TweenService")
    local tweenInfo = TweenInfo.new(time, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
    local tween = TweenService:Create(HumanoidRootPart, tweenInfo, {CFrame = destination})
    tween:Play()
    HumanoidRootPart.Anchored = false
end

------------------------------------------------------------------------------------------------------------------------------------

Tab1:AddButton({
    Name = "Shop",
    Callback = function(value)
        if SMB then
            TPSMB(CFrame.new(4320.8154296875, -155.29226684570312, 6378.77880859375))
        end
    end
})

------------------------------------------------------------------------------------------------------------------------------------

Tab1:AddButton({
    Name = "BlackSmith",
    Callback = function(value)
        if SMB then
            TPSMB(CFrame.new(211.25051879882812, 18.948881149291992, -368.9964294433594))
        end
    end
})

